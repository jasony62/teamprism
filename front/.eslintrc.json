{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "extends": ["plugin:vue/essential", "eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "rules": {
    "semi": "off"
  }
}
